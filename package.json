{
  "name": "tms",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:watch": "cross-env NODE_ENV=development nest start --watch",
    "start:prod:watch": "cross-env NODE_ENV=production nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/db/dataSource.ts",
    "typeorm:dev": "typeorm-ts-node-commonjs -d ./src/db/dataSource.ts",
    "typeorm:prod": "typeorm-ts-node-commonjs -d ./dist/db/dataSource.js",
    "mi:dev:g": "cross-env NODE_ENV=development yarn run typeorm:dev migration:generate ./dist/migrations/migration-dev -o",
    "mi:prod:g": "cross-env NODE_ENV=production yarn run typeorm:prod migration:generate ./dist/migrations/migration-prod -o",
    "mi:dev:r": "cross-env NODE_ENV=development yarn run typeorm:dev migration:run",
    "mi:prod:r": "cross-env NODE_ENV=production yarn run typeorm:prod migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/lib-storage": "^3.821.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@aws-sdk/types": "^3.821.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.3",
    "@romanize/korean": "^0.1.3",
    "@types/chroma-js": "^2.4.4",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "archiver": "^7.0.1",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.53.1",
    "chroma-js": "^2.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "es-hangul": "^2.3.3",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sanitize-filename": "^1.6.3",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/archiver": "^6.0.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.17",
    "@types/mime-types": "^3.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport": "^1.0.17",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
